# SuperClaw Environment Configuration
# Copy this to .env and fill in your values
# The setup wizard (setup.py) generates this automatically

# ── Database ──────────────────────────────────────
POSTGRES_USER=superclaw
POSTGRES_PASSWORD=CHANGE_ME_GENERATE_A_REAL_PASSWORD
POSTGRES_DB=superclaw
POSTGRES_PORT=5432

# ── n8n Workflow Engine ───────────────────────────
N8N_ENCRYPTION_KEY=CHANGE_ME_GENERATE_A_REAL_KEY
N8N_JWT_SECRET=CHANGE_ME_GENERATE_A_REAL_SECRET
N8N_PORT=5678

# ── Qdrant Vector Store ──────────────────────────
QDRANT_HOST=localhost
QDRANT_PORT=6333

# ── Ollama (local LLM runtime) ───────────────────
OLLAMA_URL=http://localhost:11434
EMBED_MODEL=nomic-embed-text

# ── Bridges ──────────────────────────────────────
LAZARUS_PORT=8888
HERMES_PORT=8787
POSTGRES_DSN=postgresql://superclaw:CHANGE_ME@localhost:5432/superclaw
N8N_BASE_URL=http://localhost:5678
HERMES_URL=http://localhost:8787
# QUIET_TZ=America/Chicago
# HERMES_CORS_ORIGINS=*            # Comma-separated origins, or * for all

# ── Advanced Tuning (optional) ───────────────────
# These have sensible defaults. Only set if you need to override.
# CLAUDE_BIN=claude                # Path to Claude CLI binary
# SUPERCLAW_BIN=openclaw            # Path to SuperClaw runtime binary
# CLAUDE_TIMEOUT=120               # Seconds before Claude calls time out
# AGENT_TIMEOUT=120                # Seconds before agent calls time out
# HERMES_TARGET=claude             # Default voice target (claude or agent name)
# HERMES_POLICY=bridge/hermes_policy.json  # Policy file path

# ── SuperClaw Runtime ────────────────────────────
SUPERCLAW_PORT=18789

# ── Voice & Discord (optional) ───────────────────
# Uncomment and fill in to enable voice features.
# Run 'bash scripts/setup-voice.sh' first to install dependencies.
# WHISPER_MODEL=base
# WHISPER_DEVICE=cuda
# TTS_VOICE=en-US-AriaNeural
# VAD_SENSITIVITY=2
# VOICE_PORT=8686
# DISCORD_BOT_TOKEN=
# DEFAULT_AGENT=coordinator
